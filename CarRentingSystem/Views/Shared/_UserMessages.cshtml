@using CarRentingSystem.Constants

@if(TempData[AgentControllerConstants.ErrorMessage] != null)
{
    <script>
        $(function () 
        {
            toastr.error('@TempData[AgentControllerConstants.ErrorMessage].ToString()');
        });
    </script>
}

@if (TempData[AgentControllerConstants.WarningMessage] != null)
{
    <script>
        $(function () {
            toastr.warning('@TempData[AgentControllerConstants.WarningMessage].ToString()');
        });
    </script>
}

@if (TempData[AgentControllerConstants.SuccessMessage] != null)
{
    <script>
        $(function () {
            toastr.success('@TempData[AgentControllerConstants.SuccessMessage].ToString()');
        });
    </script>
}